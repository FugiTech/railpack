{
  "$schema": "../../test/schema.json",
  "buildAptPackages": ["cowsay"],
  "steps": {
    "build": {
      "commands": [{ "path": "/usr/games" }, "cowsay hello"]
    }
  },
  "deploy": {
    "inputs": [
      "...",
      { "image": "macabees/neofetch", "include": ["/usr/bin/neofetch"] }
    ],
    "aptPackages": ["cowsay"],
    "startCommand": "neofetch && cowsay hello"
  }
}
