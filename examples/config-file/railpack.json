{
  "$schema": "../../test/schema.json",
  "provider": "node",
  "packages": {
    "python": "latest"
  },
  "buildAptPackages": ["neofetch"],
  "steps": {
    "build": {
      "commands": ["neofetch"],
      "variables": {
        "HELLO": "world"
      }
    }
  },
  "deploy": {
    "aptPackages": ["neofetch"],
    "startCommand": "python --version && neofetch $HELLO"
  }
}
