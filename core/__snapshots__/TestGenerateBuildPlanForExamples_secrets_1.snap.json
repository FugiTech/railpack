{
 "baseImage": "ghcr.io/railwayapp/railpack-runtime-base:latest",
 "secrets": [
  "MY_SECRET",
  "MY_OTHER_SECRET",
  "HELLO_WORLD"
 ],
 "start": {
  "cmd": "./run.sh"
 },
 "steps": [
  {
   "name": "packages:mise",
   "secrets": [
    "*"
   ]
  },
  {
   "commands": [
    {
     "dest": ".",
     "src": "."
    },
    {
     "cmd": "sh -c './run.sh'",
     "customName": "./run.sh"
    }
   ],
   "dependsOn": [
    "packages:mise"
   ],
   "name": "defaultsToUsing",
   "outputs": [
    "/app"
   ],
   "secrets": [
    "*"
   ],
   "variables": {
    "NOT_SECRET": "not secret"
   }
  },
  {
   "commands": [
    {
     "dest": ".",
     "src": "."
    },
    {
     "cmd": "sh -c './run.sh true'",
     "customName": "./run.sh true"
    }
   ],
   "dependsOn": [
    "packages:mise"
   ],
   "name": "doesNotUseSecrets",
   "outputs": [
    "/app"
   ],
   "secrets": []
  },
  {
   "commands": [
    {
     "dest": ".",
     "src": "."
    },
    {
     "cmd": "sh -c './run.sh'",
     "customName": "./run.sh"
    }
   ],
   "dependsOn": [
    "packages:mise"
   ],
   "name": "usesSecrets",
   "outputs": [
    "/app"
   ],
   "secrets": [
    "MY_SECRET",
    "MY_OTHER_SECRET"
   ],
   "variables": {
    "NOT_SECRET": "not secret"
   }
  }
 ]
}