{
 "baseImage": "ghcr.io/railwayapp/railpack-runtime-base:latest",
 "secrets": [
  "MY_SECRET",
  "MY_OTHER_SECRET",
  "HELLO_WORLD"
 ],
 "start": {
  "cmd": "./run.sh"
 },
 "steps": [
  {
   "name": "packages:mise"
  },
  {
   "commands": [
    {
     "cmd": "sh -c '{ \"name\": \"NOT_SECRET\", \"value\": \"not secret\" }'",
     "customName": "{ \"name\": \"NOT_SECRET\", \"value\": \"not secret\" }"
    },
    {
     "dest": ".",
     "src": "."
    },
    {
     "cmd": "sh -c './run.sh'",
     "customName": "./run.sh"
    }
   ],
   "dependsOn": [
    "packages:mise"
   ],
   "name": "defaultsToUsing"
  },
  {
   "commands": [
    {
     "dest": ".",
     "src": "."
    },
    {
     "cmd": "sh -c './run.sh true'",
     "customName": "./run.sh true"
    }
   ],
   "dependsOn": [
    "packages:mise"
   ],
   "name": "doesNotUseSecrets",
   "useSecrets": false
  },
  {
   "commands": [
    {
     "cmd": "sh -c '{ \"name\": \"NOT_SECRET\", \"value\": \"not secret\" }'",
     "customName": "{ \"name\": \"NOT_SECRET\", \"value\": \"not secret\" }"
    },
    {
     "dest": ".",
     "src": "."
    },
    {
     "cmd": "sh -c './run.sh'",
     "customName": "./run.sh"
    }
   ],
   "dependsOn": [
    "packages:mise"
   ],
   "name": "usesSecrets"
  }
 ]
}